{
  "name": "EmCasa",
  "version": "1.0.1",
  "private": true,
  "rnpm": {
    "assets": ["./assets/fonts"]
  },
  "scripts": {
    "postinstall":
      "sed -i '' '/RUN_CLANG_STATIC_ANALYZER/d;/CLANG_STATIC_ANALYZER_MODE/d;/CLANG_ANALYZER_NONNULL/d' ./node_modules/react-native/React/React.xcodeproj/project.pbxproj",
    "prepare": "patch-package",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:log": "yarn android && yarn log-android",
    "ios:log": "yarn ios && yarn log-ios",
    "devtools": "react-devtools",
    "log-android": "react-native log-android",
    "log-ios": "react-native log-ios",
    "test": "node node_modules/jest/bin/jest.js"
  },
  "jest": {
    "preset": "react-native",
    "automock": false,
    "unmockedModulePathPatterns": ["/node_modules/"],
    "setupFiles": ["./__test__/setup.js"],
    "transformIgnorePatterns": [
      "/node_modules/(?!(jest-)?react-native|react-navigation)/"
    ],
    "modulePathIgnorePatterns": ["/redux/__mocks__", "/lib/graphql/__mocks__"]
  },
